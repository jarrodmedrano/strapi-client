{
  "env": {
    "browser": true,
    "es2021": true
  },
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "**/*/*.ts", "**/*/*.tsx"],
      "extends": [
        "airbnb-base",
        "airbnb-typescript",
        "prettier",
        "plugin:@next/next/recommended"
      ]
    }
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": ["tsconfig.json"]
  },
  "plugins": ["react", "@typescript-eslint"],
  "rules": {
    "no-console": ["error"]
  }
}
